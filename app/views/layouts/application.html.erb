<!DOCTYPE html>
<html>
<head>
  <title>Srq</title>
  <%= stylesheet_link_tag 'css.css' %>
  <%= javascript_include_tag 'jquery.js', 'rails.js', 'application', 'nested_form' %>
  <%= csrf_meta_tag %>
</head>
<body>



<div class="title_box">
<h1>SRQ v2.0</h1>
</div>



<div class="actions_bar">

   <ul>
     <li>
        <%= link_to 'Log Out', log_out_path %>
     </li>
   </ul>
</div>

<div class="core">

    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>

    <%= yield %>

</div>




</body>
</html>
