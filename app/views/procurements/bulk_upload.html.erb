<div id='container' >
    <%=  form_tag( { :controller => :procurements, :action => :bulk_process, :method => :post }, { :remote => true, :id => 'new-procurement-upload-form'} ) do %>
        <div class="box">
            <label>
                <span>File</span>
                <%= file_field_tag :attachment %>
            </label>
        </div>
        <div class="box">
            <label>
            <%= submit_tag 'Upload', :class => 'box_button ui-button ui-widget ui-state-default ui-corner-all' %>
            </label>
        </div>

    <% end %>

</div>
